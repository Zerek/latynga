{
    "manifest_version": 2,
    "name": "Zerek Latynga",
    "version": "0.1",
    "browser_action": {
        "default_icon": "icon.png"
      },
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": ["jquery-3.2.1.slim.min.js", "content.js"]
      }
    ],
    "background": {
        "scripts": ["background.js"]
      }
  }